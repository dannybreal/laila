{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "OPENAI_API_KEY": "@openai_api_key",
        "OPENAI_ASSISTANT_ID": "@openai_assistant_id"
    }
} 